include ../../sys/pug/vars

footer.footer
	.case
		.footer__main.row
			.footer__info
				.footer__logo.logo
					.logo__img
						img(src="_/uploads/logo.png" srcset="_/uploads/logo-x2.png 2x", alt="")
					.logo__text Сохраняем тепло
				.footer__req
					p ООО «ПКФ ДИНАМИКА»
					p ОГРН 1145249004581
					p ИНН 5249135654
			.footer__nav
				.footer__title Навигация по сайту
				.footer__nav-list
					ul
						li
							a(href="#") О теплоносителе
						li
							a(href="#") Наша продукция
						li
							a(href="#") Дилерам
					ul
						li
							a(href="#") Почему мы
						li
							a(href="#") Сертификаты
						li
							a(href="#") Контакты
			.footer__act
				.footer__title Остались вопросы?
				.footer__text Оставьте заявку и мы свяжемся #[br]с вами для конусльтации
				a.footer__btn.btn.--red(data-fancybox data-src="#modal-callback" href="javascript:;") Перезвоните мне
		.footer__panel.row
			.footer__panel-item @2022 Все права защищены
			a.footer__panel-item(href="politika.pdf" target="_blank") Политика конфиденциальности
			a.footer__panel-item(href="politika.pdf" target="_blank") Согласие на обработку персональных данных
		
.modal.--static#modal-callback
	.modal__wrap
		.modal__info.tac
			.modal__title Обратный звонок
			.modal__subtitle Заполните форму и мы перезвоним вам #[br]в течение 15 минут
		form.modal__form
			.modal__fields
				.modal__field.field.--required.--name
					.field__wrap
						.field__icon
							img(src="_/uploads/icons/person-gray.svg", alt="")
						input.field__area(type="text" name="name" placeholder="Введите ваше имя")
				.modal__field.field.--required.--tel
					.field__wrap
						.field__icon
							img(src="_/uploads/icons/tel-gray.svg", alt="")
						input.field__area(type="tel" name="tel" placeholder="+7 (___) ___-__-__")
				button.modal__btn.btn.--red Перезвоните мне
			.modal__policy Нажимая на кнопку вы соглашаетесь с #[a(href="politika.pdf" target="_blank") условиями обработки данных] и #[a(href="politika.pdf" target="_blank") политикой конфиденциальности]

